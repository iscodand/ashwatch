{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Go: Debug ashwatch-query",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/ashwatch-query",
      "cwd": "${workspaceFolder}/ashwatch-query"
    },
    {
      "name": "Go: Debug arquivo atual",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${fileDirname}"
    },
    {
      "name": "Go: Attach processo local",
      "type": "go",
      "request": "attach",
      "mode": "local",
      "processId": 0
    }
  ]
}
